<Window x:Class="Client.StatisticsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Client"
        mc:Ignorable="d"
        Title="StatisticsWindow" Height="325" Width="300">
	<StackPanel>
		<TextBlock HorizontalAlignment="Center" FontSize="18" FontWeight="Bold" Margin="0,5,0,5" Name="Title">
		</TextBlock>
		<TextBlock Name="Statistics" HorizontalAlignment="Center" FontSize="18"/>
		<Button Name="CloseWindow" Click="CloseWindow_Click" FontSize="16" Width="100" Margin="20">Return</Button>
	</StackPanel>
</Window>
